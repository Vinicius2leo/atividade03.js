const nomes = ["joão","helio","eduardo","paulinho","lorrayne",];

const terceironome = nomes [2];

console.log (terceironome);

nomes.push("Fábio");

nomes.unshift("Gabriel");

console.log(nomes);

nomes.pop();

console.log(nomes);

const valores = [2, 4, 6, 8];

const valoresDobrados = valores.map(valor => valor * 2);

console.log(valoresDobrados);

const numeros = [1, 3, 5, 7, 9];

const maioresQueCinco = numeros.filter(numero => numero > 5);

console.log(maioresQueCinco);
